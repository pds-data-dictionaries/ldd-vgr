  <xs:simpleType name="edit_mode_id">
    <xs:annotation>
      <xs:documentation>Edit mode of the camera, indicates amount of
        data read from the vidicon. '1:1' indicates the full-resolution
        of the vidicon. Other values include '3:5', '3:4', '1:2', '1:3',
        '1:5', and '1:10'. Source Data Product Origin: Keyword
        EDIT_MODE_ID</xs:documentation>
    </xs:annotation>
    <xs:restriction base="pds:ASCII_Short_String_Collapsed">
     <xs:minLength value="1"/>
     <xs:maxLength value="255"/>
	   </xs:restriction>
  </xs:simpleType>

